<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MarcialFernandez.GreenPlay.ListaDescargasController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #1f773c;">
         <children>
            <Label prefHeight="0.0" prefWidth="144.0" text="GreenPlay" textFill="WHITE">
               <font>
                  <Font name="Arial Bold Italic" size="29.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <TableView fx:id="tablaDescargas" prefHeight="334.0" prefWidth="600.0">
               <columns>
                  <TableColumn fx:id="fecha" prefWidth="173.33334350585938" text="Fecha" />
                  <TableColumn fx:id="multimedia" prefWidth="425.33331298828125" text="Multimedia" />
               </columns>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TableView>
            <HBox prefHeight="35.0" prefWidth="211.0">
               <children>
                  <Button fx:id="volver" mnemonicParsing="false" onAction="#volver" prefHeight="35.0" prefWidth="106.0" text="Volver" />
                  <Button fx:id="borrar" mnemonicParsing="false" onAction="#borrar" prefHeight="35.0" prefWidth="106.0" text="Borrar">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
