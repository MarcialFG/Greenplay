<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="panelPeliculas" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MarcialFernandez.GreenPlay.PeliculasController">
   <children>
      <TableView fx:id="tablaPeliculas" prefHeight="500.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="id" prefWidth="123.99993896484375" text="id" />
            <TableColumn minWidth="-1.0" prefWidth="121.33334350585938" text="Titulo" />
          <TableColumn prefWidth="139.3333740234375" text="Descripcion" />
          <TableColumn prefWidth="138.0" text="Autor" />
            <TableColumn prefWidth="74.6666259765625" text="Genero" />
        </columns>
      </TableView>
      <Button fx:id="Descargar" mnemonicParsing="false" prefHeight="27.0" prefWidth="93.0" text="Descargar">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" left="250.0" right="250.0" top="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
